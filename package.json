{
  "name": "recipeek",
  "version": "1.0.0",
  "description": "Web app to quickly search and save cooking recipes and display the pertinent nutritional info in the form of keywords to the user in small snippets to allow quick review of recipes.",
  "main": "index.js",
  "scripts": {
    "start:express": "nodemon server/apiServer.js",
    "start:dev": "export NODE_ENV=development && node server.js",
    "bundle": "webpack --progress -p --config webpack.prod.config.js",
    "build": "npm run bundle && npm run start:express && open dist/index.html"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbchoa/recipeek.git"
  },
  "author": "Michael-Bryant Choa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mbchoa/recipeek/issues"
  },
  "homepage": "https://github.com/mbchoa/recipeek#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "cheerio": "^0.20.0",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "jquery": "^2.2.4",
    "lodash": "^4.16.4",
    "nlp_compromise": "^5.1.0",
    "nodemon": "^1.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "1.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.72.0",
    "whatwg-fetch": "^1.0.0",
    "worker-farm": "^1.3.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "react-hot-loader": "1.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0"
  }
}
