{
  "name": "recipeek",
  "version": "1.0.0",
  "description": "Web app to quickly search and save cooking recipes and display the pertinent nutritional info in the form of keywords to the user in small snippets to allow quick review of recipes.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf coverage",
    "test": "mocha './{,!(node_modules)/**/}*.spec.js'",
    "coverage": "npm run clean && cross-env NODE_ENV=test nyc npm test",
    "start:dev": "export NODE_ENV=development && babel-node server.js",
    "start": "gulp build && node dist/apiServer.js"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "api-clients/**/*.js",
      "helpers/**/*.js",
      "redux/actions/**/*.js",
      "redux/reducers/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "node_modules"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "text-summary",
      "text",
      "html"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbchoa/recipeek.git"
  },
  "author": "Michael-Bryant Choa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mbchoa/recipeek/issues"
  },
  "homepage": "https://github.com/mbchoa/recipeek#readme",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.18.0",
    "cheerio": "^0.20.0",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "http-status-codes": "^1.0.6",
    "jquery": "^2.2.4",
    "lodash": "^4.16.4",
    "mocha": "^3.1.2",
    "nlp_compromise": "^5.1.0",
    "nodemon": "^1.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "1.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.78.0",
    "tcomb": "^3.2.15",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4",
    "whatwg-fetch": "^1.0.0",
    "worker-farm": "^1.3.1"
  },
  "devDependencies": {
    "artillery": "^1.5.0-17",
    "babel-cli": "^6.18.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cross-env": "^3.1.3",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^3.1.2",
    "mocha-junit-reporter": "^1.12.1",
    "nyc": "^8.4.0",
    "react-hot-loader": "1.3.0",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.6",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0"
  }
}
