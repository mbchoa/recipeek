{
  "name": "recipeek",
  "version": "1.0.0",
  "description": "Web app to quickly search and save cooking recipes and display the pertinent nutritional info in the form of keywords to the user in small snippets to allow quick review of recipes.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf coverage",
    "test": "npm run lint && jest",
    "watch": "jest --watch",
    "lint": "eslint src",
    "dev": "export NODE_ENV=development && gulp dev",
    "start": "gulp build && nodemon dist/apiServer.js"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react",
      [
        "@babel/stage-2",
        {
          "decoratorsLegacy": true
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "react-hot-loader/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbchoa/recipeek.git"
  },
  "author": "Michael-Bryant Choa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mbchoa/recipeek/issues"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "homepage": "https://github.com/mbchoa/recipeek#readme",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@babel/preset-env": "^7.0.0-beta.36",
    "@babel/preset-react": "^7.0.0-beta.36",
    "@babel/preset-stage-2": "^7.0.0-beta.36",
    "apollo-client-preset": "^1.0.5",
    "apollo-server-express": "^1.3.2",
    "axios": "^0.17.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^6.18.0",
    "body-parser": "^1.18.2",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "css-loader": "^0.28.8",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^2.14.1",
    "gulp": "^3.9.1",
    "http-status-codes": "^1.0.6",
    "lodash": "^4.16.4",
    "nodemon": "^1.9.2",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^15.5.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^15.5.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "request": "^2.78.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-dev-server": "^2.9.7",
    "webpack-node-externals": "^1.5.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "cross-env": "^3.1.3",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.0.4",
    "react-test-renderer": "^15.6.1"
  }
}
